{
    "name": "StyleTTS 2",
    "description": "Use StyleTTS 2 to narrate articles, text, and more!",
    "version": "0.1",
    "manifest_version": 3,
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "contextMenus",
        "scripting",
        "storage"
    ],
    "icons": {
        "16": "logo-16.png",
        "32": "logo-32.png",
        "48": "logo-48.png",
        "128": "logo-128.png"
    }
}
